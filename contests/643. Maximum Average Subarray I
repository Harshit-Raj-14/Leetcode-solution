class Solution {
    public double findMaxAverage(int[] nums, int k) {
        double ans=Long.MIN_VALUE;
        for(int i=0;i<=nums.length-k;i++){
            int sum=0;
            for(int j=i;j<i+k;j++){
                sum+=nums[j];
            }
            ans = Math.max(ans, sum/(k*1.0));
        }
        return ans;
    }
}
