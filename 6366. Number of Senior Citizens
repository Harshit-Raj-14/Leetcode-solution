class Solution {
    public int countSeniors(String[] details) {
        int count=0;
        for(int i=0;i<details.length;i++){
            int age = (details[i].charAt(11) - '0')*10 + (details[i].charAt(12) - '0');     //get age in proper format of int form string char to int remember this
            if(age>60) count++;
        }
        //System.out.println
        return count;
    }
}



